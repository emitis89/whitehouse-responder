{
    "name": "emyoutis/whitehouse-responder",
    "description": "A simple API for generating responses in WhiteHouse standards.",
    "type": "library",
    "keywords": [
        "API",
        "WhiteHouse"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Emyoutis Yousefi",
            "email": "emitis89@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Emyoutis\\": "src/"
        }
    },
    "require": {
        "php": ">=7.2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Emyoutis\\WhiteHouseResponder\\Laravel\\WhiteHouseResponderServiceProvider"
            ],
            "aliases": {
                "WhiteHouseResponderResponse": "Emyoutis\\WhiteHouseResponder\\Laravel\\Facades\\Response",
                "WhiteHouseResponderErrors": "Emyoutis\\WhiteHouseResponder\\Laravel\\Facades\\ErrorsRepository"
            }
        }
    }
}
